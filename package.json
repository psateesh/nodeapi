  "scripts": {
    "prebuild": "npm run build:clean",
    "build": "webpack --config internals/webpack/webpack.dev.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development node server",
    "server:local": "cross-env NODE_ENV=development nodemon server",
    "clean": "yarn run test:clean && yarn run build:clean",
    "fetchMasterData": "node server/fetchMasterData.js",
    "storeMasterDataInRedis": "node server/storeMasterDataInRedis.js",
    "lint": "yarn run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "eslint:fix": "eslint -- . --ignore-path .gitignore --fix",
    "lint:js": "yarn run lint:eslint -- . ",
    "pretest": "yarn run test:clean && yarn run lint",
    "test:clean": "rimraf ./coverage",
    "test": "test.js ",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "fix-code": "prettier-eslint --write 'src/**/*.{js,jsx}'",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js  /app/SignIn/tests/Signin.test.js --runInBand",
    "clear": "rm -rf yarn.lock && rm -rf node_modules",
    "reinstall": "yarn run clear && yarn install",
    "analyzeProdBundle": "webpack-bundle-analyzer .logs/stats.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "js-docs": "jsdoc -c jsdoc.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{scss, css}": [
      "stylefmt",
      "stylelint --allow-empty-input --fix",
      "git add"
    ],
    "app/**/*.{js,jsx}": [
      "prettier --write",
      "./node_modules/.bin/eslint --fix",
      "git add"
    ],
    "server/**/*.js": [
      "prettier --write",
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
